<tr class="search-result" style="cursor:default;">
    <td>
        {{#with j}}
        <div class="row jira">
            <input class="jira-key" type="hidden" value="{{this}}">

            <div class="col-xs-2 jira-id">
                <a href="{{jiraLink}}" target="_blank">
                    i
                </a>
            </div>{{~space ~}}
            <div class="col-xs-6 jira-name">n</div>{{~space ~}}
            <div class="col-xs-2 jira-status" ><span>{{status}}</span></div>{{~space ~}}
            <div class="col-xs-2 jira-assignee">{{nameFormat assignee}}</div>{{~space ~}}


        </div>
        {{/with}}
        {{#each a}}
        <input class="asana_id" type="hidden" value="{{this}}">
        {{#asana}}
        <div class="row asana">{{~space ~}}
            <div class="col-xs-2 asana-link">
                <a href="{{asanaLink}}" target="_blank"><span class="glyphicon glyphicon-link"></span></a>
            </div>{{~space ~}}
            <div class="col-xs-8 asana-name">
                {{name}}
            </div>{{~space ~}}
            <div class="col-xs-2 asana-assignee">{{nameFormat assignee}}</div>{{~space ~}}
        </div>

        {{/asana}}
        {{/each}}
    </td>
</tr>
